{
    "name": "owowagency/laravel-notifications",
    "description": "A package of notifications features for the Laravel framework.",
    "type": "library",
    "authors": [
        {
            "name": "Albert Pratomo",
            "email": "albert@owow.io"
        }
    ],
    "keywords": [
        "Laravel",
        "Notifications"
    ],
    "require": {
        "php": "^7.4",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "owowagency/laravel-test-response": "^0.0.5",
        "owowagency/phpunit-snapshot-assertions": "^0.0.8"
    },
    "autoload": {
        "psr-4": {
            "OwowAgency\\LaravelNotifications\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OwowAgency\\LaravelNotifications\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OwowAgency\\LaravelNotifications\\LaravelNotificationsServiceProvider"
            ]
        }
    }
}
